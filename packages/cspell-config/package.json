{
  "name": "@w5s/cspell-config",
  "version": "1.0.8",
  "description": "CSpell configuration presets",
  "keywords": [
    "cspell"
  ],
  "homepage": "https://github.com/w5s/project-config/blob/main/packages/cspell-config#readme",
  "bugs": {
    "url": "https://github.com/w5s/project-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:w5s/project-config.git",
    "directory": "packages/cspell-config"
  },
  "license": "MIT",
  "author": "Julien Polo <julien.polo@gmail.com>",
  "type": "commonjs",
  "exports": {
    ".": "./cspell-ext.json",
    "./cspell-ext.json": "./cspell-ext.json",
    "./lib/*": "./lib/*",
    "./dict/*": "./dict/*"
  },
  "typings": "./index.d.ts",
  "files": [
    "cspell-ext.json",
    "dict/**/*",
    "lib/**/!(*.spec).d.ts",
    "lib/**/!(*.spec).d.ts.map",
    "lib/**/!(*.spec).js.map",
    "lib/**/!(*.spec).js",
    "src/**/!(*.spec).ts"
  ],
  "scripts": {
    "__build:dict": "cd dict;cspell-tools-cli compile filetypes.txt",
    "build": "concurrently \"npm:build:*\" \":\"",
    "build:tsc": "tsc -b tsconfig.build.json",
    "clean": "concurrently \"npm:clean:*\" \":\"",
    "clean:tsc": "rm -rf lib",
    "docs": "node ../../markdown.mjs",
    "format": "concurrently \"npm:format:*\" \":\"",
    "format:src": "eslint . --fix --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "lint": "concurrently \"npm:lint:*\" \":\"",
    "lint:src": "eslint . --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "prepare": "concurrently \"npm:prepare:*\" \":\"",
    "prepublishOnly": "npm run clean;npm run build",
    "spellcheck": "cspell --no-progress '**'",
    "test": "concurrently \"npm:test:*\" \":\"",
    "test:src": "vitest run"
  },
  "dependencies": {
    "@cspell/cspell-bundled-dicts": "^6.0.0"
  },
  "devDependencies": {
    "@cspell/cspell-tools": "6.31.1",
    "@cspell/cspell-types": "6.31.1",
    "vite": "4.3.9",
    "vitest": "0.31.1"
  },
  "peerDependencies": {
    "cspell": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "cspell": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
