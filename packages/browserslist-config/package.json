{
  "name": "@w5s/browserslist-config",
  "version": "1.0.0-alpha.8",
  "description": "Browserslist support shared configuration",
  "keywords": [
    "browserslist",
    "config"
  ],
  "homepage": "https://github.com/w5s/project-config/blob/master/packages/browserslist-config#readme",
  "bugs": {
    "url": "https://github.com/w5s/project-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:w5s/project-config.git",
    "directory": "packages/browserslist-config"
  },
  "license": "MIT",
  "author": "Julien Polo <julien.polo@gmail.com>",
  "type": "commonjs",
  "exports": {
    ".": "./index.js",
    "./lib/*": "./lib/*"
  },
  "typings": "./index.d.ts",
  "files": [
    "lib/**/!(*.spec).d.ts",
    "lib/**/!(*.spec).d.ts.map",
    "lib/**/!(*.spec).js.map",
    "lib/**/!(*.spec).js",
    "src/**/!(*.spec).ts",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "build": "concurrently \"npm:build:*\" \":\"",
    "build:tsc": "tsc;yarn docs",
    "clean": "concurrently \"npm:clean:*\" \":\"",
    "clean:src": "rm -rf lib/*",
    "docs": "node scripts/build-docs.mjs && md-magic --path '**/*.md' --ignore='node_modules'",
    "format": "concurrently \"npm:format:*\" \":\"",
    "format:src": "eslint . --fix --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "lint": "concurrently \"npm:lint:*\" \":\"",
    "lint:src": "eslint . --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "prepare": "concurrently \"npm:prepare:*\" \":\"",
    "spellcheck": "cspell --no-progress '**'",
    "test": "concurrently \"npm:test:*\" ",
    "test:src": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "preset": "es-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/build/",
      "/.cache/",
      "/docs/",
      "/public/"
    ]
  },
  "devDependencies": {
    "@jest/globals": "29.4.0",
    "browserslist": "4.21.4",
    "lodash-es": "4.17.21"
  },
  "peerDependencies": {
    "browserslist": ">= 4.0"
  },
  "peerDependenciesMeta": {
    "browserslist": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
