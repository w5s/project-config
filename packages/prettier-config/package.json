{
  "name": "@w5s/prettier-config",
  "version": "1.1.1",
  "description": "Prettier shared configuration",
  "keywords": [
    "typescript",
    "config",
    "tsc"
  ],
  "homepage": "https://github.com/w5s/project-config/blob/main/packages/prettier-config#readme",
  "bugs": {
    "url": "https://github.com/w5s/project-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:w5s/project-config.git",
    "directory": "packages/prettier-config"
  },
  "license": "MIT",
  "author": "Julien Polo <julien.polo@gmail.com>",
  "type": "commonjs",
  "exports": {
    ".": "./index.js",
    "./dist/*": "./dist/*"
  },
  "typings": "./index.d.ts",
  "files": [
    "dist/",
    "src/",
    "index.js",
    "index.d.ts",
    "!**/*.spec.*",
    "!**/__tests__/**"
  ],
  "scripts": {
    "build": "concurrently \"npm:build:*\" \":\"",
    "build:tsc": "tsc -b tsconfig.build.json",
    "clean": "concurrently \"npm:clean:*\" \":\"",
    "clean:tsc": "rm -rf dist",
    "docs": "node ../../markdown.mjs",
    "format": "concurrently \"npm:format:*\" \":\"",
    "format:src": "eslint . --fix --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "lint": "concurrently \"npm:lint:*\" \":\"",
    "lint:src": "eslint . --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "prepare": "concurrently \"npm:prepare:*\" \":\"",
    "prepublishOnly": "npm run clean;npm run build",
    "spellcheck": "cspell --no-progress '**'",
    "test": "concurrently \"npm:test:*\" \":\"",
    "test:src": "vitest run"
  },
  "devDependencies": {
    "prettier": "3.0.0",
    "vite": "4.4.3",
    "vitest": "0.33.0"
  },
  "peerDependencies": {
    "prettier": "2.x || 3.x"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
