{
  "name": "@w5s/prettier-config",
  "version": "2.2.11",
  "description": "Prettier shared configuration",
  "keywords": [
    "typescript",
    "config",
    "tsc"
  ],
  "homepage": "https://github.com/w5s/project-config/blob/main/packages/prettier-config#readme",
  "bugs": {
    "url": "https://github.com/w5s/project-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:w5s/project-config.git",
    "directory": "packages/prettier-config"
  },
  "license": "MIT",
  "author": "Julien Polo <julien.polo@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist/",
    "src/",
    "index.js",
    "index.d.ts",
    "!*.d.ts.map",
    "!**/*.spec.*",
    "!**/__tests__/**"
  ],
  "scripts": {
    "build": "npx run-p \"build:*\"",
    "build:tsc": "npx tsdown",
    "clean": "npx run-p \"clean:*\"",
    "clean:tsc": "rm -rf dist",
    "docs": "node ../../markdown.mjs",
    "format": "npx run-p \"format:*\"",
    "format:src": "eslint . --fix",
    "lint": "npx run-p \"lint:*\"",
    "lint:src": "eslint .",
    "postpack": "npx clean-package restore",
    "prepack": "npx clean-package",
    "prepare": "npx run-p \"prepare:*\"",
    "prepare:empty": ":",
    "prepublishOnly": "npm run clean;npm run build",
    "spellcheck": "cspell --no-progress '**'",
    "test": "npx run-p \"test:*\"",
    "test:src": "vitest run"
  },
  "devDependencies": {
    "@w5s/tsdown-config": "workspace:*",
    "prettier": "3.6.2",
    "tsdown": "0.15.9",
    "vite": "7.1.11",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "prettier": "2.x || 3.x"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "clean-package": "../../clean-package.config.cjs",
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffect": false
}
