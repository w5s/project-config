{
  "name": "@w5s/prettier-config",
  "version": "2.0.2",
  "description": "Prettier shared configuration",
  "keywords": [
    "typescript",
    "config",
    "tsc"
  ],
  "homepage": "https://github.com/w5s/project-config/blob/main/packages/prettier-config#readme",
  "bugs": {
    "url": "https://github.com/w5s/project-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:w5s/project-config.git",
    "directory": "packages/prettier-config"
  },
  "license": "MIT",
  "author": "Julien Polo <julien.polo@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "src/",
    "index.js",
    "index.d.ts",
    "!**/*.spec.*",
    "!**/__tests__/**"
  ],
  "scripts": {
    "build": "npx concurrently \"npm:build:*\" \":\"",
    "build:tsc": "tsc && vite build",
    "clean": "npx concurrently \"npm:clean:*\" \":\"",
    "clean:tsc": "rm -rf dist",
    "docs": "node ../../markdown.mjs",
    "format": "npx concurrently \"npm:format:*\" \":\"",
    "format:src": "eslint . --fix --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "lint": "npx concurrently \"npm:lint:*\" \":\"",
    "lint:src": "eslint . --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "postpack": "npx clean-package restore",
    "prepack": "npx clean-package",
    "prepare": "npx concurrently \"npm:prepare:*\" \":\"",
    "prepublishOnly": "npm run clean;npm run build",
    "spellcheck": "cspell --no-progress '**'",
    "test": "npx concurrently \"npm:test:*\" \":\"",
    "test:src": "vitest run"
  },
  "devDependencies": {
    "@w5s/vite-config": "workspace:*",
    "prettier": "3.1.0",
    "vite": "5.0.6",
    "vitest": "0.34.6"
  },
  "peerDependencies": {
    "prettier": "2.x || 3.x"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "clean-package": "../../clean-package.config.cjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
