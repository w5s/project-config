{
  "name": "@w5s/eslint-config",
  "version": "1.0.0-alpha.16",
  "description": "ESLint configuration presets",
  "keywords": [
    "eslint",
    "config",
    "lint"
  ],
  "homepage": "https://github.com/w5s/project-config/blob/main/packages/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/w5s/project-config.git/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/w5s/project-config.git",
    "directory": "packages/eslint-config"
  },
  "license": "MIT",
  "author": "Julien Polo <julien.polo@gmail.com>",
  "main": "index.js",
  "files": [
    "*.js",
    "rules/*.js"
  ],
  "scripts": {
    "build": "npm-run-all -p 'build:*'",
    "build:empty": ":",
    "docs": "md-magic --path '**/*.md' --ignore='node_modules'",
    "format": "eslint . --quiet --fix --ext=mjs,cjs,js,jsx,ts,tsx,json",
    "lint": "eslint . --ext=mjs,cjs,js,jsx,ts,tsx,json",
    "spellcheck": "cspell '**' --silent",
    "test": "scripts/test"
  },
  "eslintConfig": {
    "rules": {
      "sort-keys-fix/sort-keys-fix": "error"
    }
  },
  "eslintIgnore": [
    "__tests__"
  ],
  "dependencies": {
    "@babel/eslint-parser": "^7.0.0",
    "@babel/plugin-syntax-class-properties": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-functional": "^4.2.0",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jsdoc": "^39.0.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-total-functions": "^5.0.0",
    "eslint-plugin-unicorn": "^42.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.18.2",
    "@types/eslint": "8.4.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/prettier": "2.4.4",
    "@types/react": "17.0.2",
    "@typescript-eslint/parser": "5.27.0",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-index": "1.5.0",
    "prettier": "2.6.2",
    "react": "18.1.0"
  },
  "peerDependencies": {
    "eslint": "5.x || 6.x || 7.x || 8.x",
    "prettier": "2.x",
    "typescript": "4.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
