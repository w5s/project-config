{
  "name": "@w5s/eslint-config",
  "version": "3.1.0",
  "description": "ESLint configuration presets",
  "keywords": [
    "eslint",
    "config",
    "lint"
  ],
  "homepage": "https://github.com/w5s/project-config/blob/main/packages/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/w5s/project-config.git/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:w5s/project-config.git",
    "directory": "packages/eslint-config"
  },
  "license": "MIT",
  "author": "Julien Polo <julien.polo@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist/",
    "src/",
    "index.js",
    "index.d.ts",
    "!*.d.ts.map",
    "!**/*.spec.*",
    "!**/__tests__/**"
  ],
  "scripts": {
    "build": "npx run-p \"build:*\"",
    "build:tsc": "node --experimental-strip-types scripts/typegen.ts && tsup",
    "clean": "npx run-p \"clean:*\"",
    "clean:tsc": "rm -rf dist",
    "docs": "node ../../markdown.mjs",
    "format": "npx run-p \"format:*\"",
    "format:src": "eslint . --fix",
    "lint": "npx run-p \"lint:*\"",
    "lint:src": "eslint .",
    "postpack": "npx clean-package restore",
    "prepack": "npx clean-package",
    "prepare": "npx run-p \"prepare:*\"",
    "prepare:empty": ":",
    "prepublishOnly": "npm run clean;npm run build",
    "spellcheck": "cspell --no-progress '**'",
    "test": "npx run-p \"test:*\"",
    "test:script": "scripts/test",
    "test_:rule-coverage": "eslint-find-rules --unused"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/eslint-plugin": "^1.0.0",
    "@w5s/dev": "^3.1.0",
    "@w5s/prettier-config": "^3.0.1",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-jsdoc": "^61.0.0",
    "eslint-plugin-jsonc": "^2.4.0",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-unicorn": "^61.0.0",
    "eslint-plugin-yml": "^1.1.0",
    "find-up": "^8.0.0",
    "globals": "^16.0.0",
    "jsonc-eslint-parser": "^2.0.0",
    "parse-gitignore": "^2.0.0",
    "yaml-eslint-parser": "^1.0.0"
  },
  "devDependencies": {
    "@types/eslint": "9.6.1",
    "@types/parse-gitignore": "1.0.2",
    "@types/react": "19.2.2",
    "@typescript-eslint/parser": "8.46.3",
    "eslint": "9.38.0",
    "eslint-find-rules": "5.0.0",
    "eslint-typegen": "2.3.0",
    "react": "19.2.0",
    "vite": "7.1.12",
    "vitest": "4.0.5"
  },
  "peerDependencies": {
    "eslint": "9.x",
    "typescript": "4.x || 5.x"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "clean-package": "../../clean-package.config.cjs",
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffect": false
}
