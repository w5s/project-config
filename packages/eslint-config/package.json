{
  "name": "@w5s/eslint-config",
  "version": "1.0.0-alpha.33",
  "description": "ESLint configuration presets",
  "keywords": [
    "eslint",
    "config",
    "lint"
  ],
  "homepage": "https://github.com/w5s/project-config/blob/main/packages/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/w5s/project-config.git/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:w5s/project-config.git",
    "directory": "packages/eslint-config"
  },
  "license": "MIT",
  "author": "Julien Polo <julien.polo@gmail.com>",
  "type": "commonjs",
  "main": "index.js",
  "files": [
    "*.js",
    "rules/*.js"
  ],
  "scripts": {
    "build": "concurrently \"npm:build:*\" \":\"",
    "clean": "concurrently \"npm:clean:*\" \":\"",
    "docs": "md-magic --path '**/*.md' --ignore='node_modules'",
    "format": "concurrently \"npm:format:*\" \":\"",
    "format:src": "eslint . --fix --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "lint": "concurrently \"npm:lint:*\" \":\"",
    "lint:src": "eslint . --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "prepare": "concurrently \"npm:prepare:*\" \":\"",
    "spellcheck": "cspell --no-progress '**'",
    "test": "concurrently \"npm:test:*\" ",
    "test:src": "scripts/test"
  },
  "eslintConfig": {
    "rules": {
      "sort-keys-fix/sort-keys-fix": "error"
    }
  },
  "eslintIgnore": [
    "__tests__"
  ],
  "dependencies": {
    "@babel/core": "*",
    "@babel/eslint-parser": "*",
    "@rushstack/eslint-patch": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-functional": "^4.2.0",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-jest": "^27.0.0",
    "eslint-plugin-jsdoc": "^39.0.0",
    "eslint-plugin-jsonc": "^2.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-total-functions": "^6.0.0",
    "eslint-plugin-unicorn": "^44.0.0",
    "eslint-plugin-yml": "^1.1.0",
    "find-up": "^5.0.0",
    "parse-gitignore": "^1.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@types/eslint": "8.4.7",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/prettier": "2.7.1",
    "@types/react": "18.0.21",
    "@typescript-eslint/parser": "5.40.1",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-index": "1.5.0",
    "prettier": "2.7.1",
    "react": "18.2.0"
  },
  "peerDependencies": {
    "eslint": "8.x",
    "prettier": "2.x",
    "typescript": "4.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
